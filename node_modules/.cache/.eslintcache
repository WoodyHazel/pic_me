"79","80","81"],[],"import React, { useContext } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faArrowLeft } from \"@fortawesome/free-solid-svg-icons\";\nimport { faHeart } from \"@fortawesome/free-solid-svg-icons\";\nimport { faHeart as farHeart } from \"@fortawesome/free-regular-svg-icons\";\nimport { Context } from \"../Context\";\n\nfunction ProductDetail() {\n  const { currentProduct, addToCart, toggleFavorited } = useContext(Context);\n\n  function favorited() {\n    if (currentProduct.liked_by_user === true) {\n      return (\n        <h4\n          onClick={() => {\n            toggleFavorited(currentProduct.id);\n            currentProduct.liked_by_user = !currentProduct.liked_by_user;\n          }}\n        >\n          Favorited!\n          <FontAwesomeIcon size=\"lg\" icon={faHeart} className=\"icon px1\" />\n        </h4>\n      );\n    } else {\n      return (\n        <h4\n          onClick={() => {\n            toggleFavorited(currentProduct.id);\n            currentProduct.liked_by_user = !currentProduct.liked_by_user;\n          }}\n        >\n          Favorite?\n          <FontAwesomeIcon size=\"lg\" icon={farHeart} className=\"icon px1\" />\n        </h4>\n      );\n    }\n  }\n  console.log(currentProduct.id);\n  return (\n    <div className=\"product-detail page\">\n      <Link to=\"/the_perfect_pic\">\n        <button className=\"btn\">\n          <FontAwesomeIcon size=\"lg\" icon={faArrowLeft} className=\"icon\" /> Back\n        </button>\n      </Link>\n      <div className=\"container\">\n        <div className=\"product-imgs\">\n          <img className=\"showcase\" src={currentProduct.urls.regular} />\n          <div>\n            <img className=\"rounded\" src={currentProduct.urls.regular} />\n            <img className=\"square\" src={currentProduct.urls.regular} />\n            <img className=\"circular\" src={currentProduct.urls.regular} />\n          </div>\n        </div>\n        <div className=\"product-info\">\n          <div className=\"creator\">\n            <h2>\n              By:{\" \"}\n              <a target=\"_blank\" href={currentProduct.user.links.html}>\n                {currentProduct.user.name}\n              </a>\n            </h2>\n            <h4>\n              Original Size:{\" \"}\n              <span>\n                {currentProduct.width}W x {currentProduct.height}H\n              </span>\n            </h4>\n          </div>\n          <div>{favorited()}</div>\n          <div className=\"download\">\n            <h3>Digital Download</h3>\n            <h4>Price: $8.99</h4>\n            <button onClick={() => addToCart(currentProduct)}>\n              Add To Cart\n            </button>\n          </div>\n          <div className=\"print\">\n            <h3>Printed Copy</h3>\n            <label htmlFor=\"size\">Size:</label>\n            <select name=\"\" id=\"size\">\n              <option value=\"\">--options--</option>\n              <option value=\"\">Original</option>\n              <option value=\"\">Small</option>\n              <option value=\"\">Medium</option>\n              <option value=\"\">Large</option>\n            </select>\n            <br />\n            <label htmlFor=\"shape\">Shape:</label>\n            <select name=\"\" id=\"shape\">\n              <option value=\"\">--options--</option>\n              <option value=\"\">Original</option>\n              <option value=\"\">Portrait</option>\n              <option value=\"\">Landscape</option>\n              <option value=\"\">Rounded</option>\n              <option value=\"\">Square</option>\n              <option value=\"\">Circular</option>\n            </select>\n            <br />\n            <label htmlFor=\"paper\">Paper:</label>\n            <select name=\"\" id=\"paper\">\n              <option value=\"\">--options--</option>\n              <option value=\"\">Stock</option>\n              <option value=\"\">High Gloss</option>\n            </select>\n            <br />\n            <label htmlFor=\"quantity\">Quantity:</label>\n            <input id=\"quantity\" type=\"number\" placeholder=\"0\" />\n            <h4>Price: $0.00</h4>\n            <button>Add To Cart</button>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default ProductDetail;\n","/Users/hannahtufts/pic_me/src/pages/About.js",[],[],"/Users/hannahtufts/pic_me/src/pages/Home.js",[],[],"/Users/hannahtufts/pic_me/src/components/Product.js",[],[],"/Users/hannahtufts/pic_me/src/components/PageSelector.js",[],[],"/Users/hannahtufts/pic_me/src/components/Form.js",["82"],[],"import React, { useState, useEffect, useContext } from \"react\";\nimport { Context } from \"../Context\";\n\nfunction Form() {\n  const { searchQuery, setSearchQuery, perPage, setPerPage, setPageCount } =\n    useContext(Context);\n\n  const [inputText, setInputText] = useState(\"\");\n\n  function handleSubmit(e) {\n    e.preventDefault();\n    setSearchQuery(inputText);\n    setPageCount(1);\n    setInputText(\"\");\n  }\n\n  return (\n    <form className=\"form\" onSubmit={handleSubmit}>\n      <div className=\"search\">\n        <label htmlFor=\"search\">Search:</label>\n        <input\n          name=\"search\"\n          id=\"search\"\n          type=\"text\"\n          value={inputText}\n          onChange={(e) => setInputText(e.target.value)}\n        />\n        {inputText.length > 0 ? (\n          <button>Submit</button>\n        ) : (\n          <button disabled>Submit</button>\n        )}\n      </div>\n      <div className=\"per-page\">\n        <label htmlFor=\"per_page\">Results per page:</label>\n        <select\n          name=\"perPage\"\n          id=\"per_page\"\n          value={perPage}\n          onChange={(e) => setPerPage(parseInt(e.target.value))}\n        >\n          <option value=\"10\">10</option>\n          <option value=\"20\">20</option>\n          <option value=\"30\">30</option>\n        </select>\n      </div>\n      <div className=\"results\">\n        {searchQuery === \"\" ? (\n          <p>Currently Viewing Featured Photos</p>\n        ) : (\n          <p>Showing Results for: \"{searchQuery}\"</p>\n        )}\n        {searchQuery && <button>Clear Results</button>}\n      </div>\n    </form>\n  );\n}\n\nexport default Form;\n","/Users/hannahtufts/pic_me/src/utils/index.js",[],[],{"ruleId":"83","severity":1,"message":"84","line":18,"column":9,"nodeType":"85","endLine":18,"endColumn":38},{"ruleId":"83","severity":1,"message":"84","line":49,"column":11,"nodeType":"85","endLine":49,"endColumn":73},{"ruleId":"83","severity":1,"message":"84","line":51,"column":13,"nodeType":"85","endLine":51,"endColumn":74},{"ruleId":"83","severity":1,"message":"84","line":52,"column":13,"nodeType":"85","endLine":52,"endColumn":73},{"ruleId":"83","severity":1,"message":"84","line":53,"column":13,"nodeType":"85","endLine":53,"endColumn":75},{"ruleId":"86","severity":1,"message":"87","line":60,"column":15,"nodeType":"85","messageId":"88","endLine":60,"endColumn":72,"fix":"89"},{"ruleId":"90","severity":1,"message":"91","line":1,"column":27,"nodeType":"92","messageId":"93","endLine":1,"endColumn":36},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" (which implies rel=\"noopener\") is a security risk in older browsers: see https://mathiasbynens.github.io/rel-noopener/#recommendations","noTargetBlankWithoutNoreferrer",{"range":"94","text":"95"},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar",[2100,2100]," rel=\"noreferrer\""]